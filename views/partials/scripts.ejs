<script>   

    const ws = new WebSocket('ws://www.seismicportal.eu/standing_order/websocket')

    ws.onopen = () => {
        console.log('ws opened on browser')
    }

    ws.onmessage = (message) => {
        console.log(message.data)
    }

    ws.onclose = (event) => {
        console.log("ws closed")
        console.log(event);
    }

    ws.onopen = () => {
        setTimeout(function() {
            ws.send("I'm still here")
        }, 300000);
        console.log("ws pinged");
    }


</script>  