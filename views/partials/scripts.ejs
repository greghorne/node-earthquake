<script>   

    const ws = new WebSocket('ws://www.seismicportal.eu/standing_order/websocket')

    ws.onopen = () => {
        console.log('ws opened on browser')
    }

    ws.onmessage = (message) => {
        console.log(`message received`, message.data)
    }

    ws.onclose = () => {
        console.log("ws closed")
    }

</script>  